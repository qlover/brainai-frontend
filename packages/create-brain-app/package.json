{
  "name": "@brain/create-brain-app",
  "description": "A node lib template",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "homepage": "",
  "author": "qlover",
  "license": "ISC",
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./cjs/*": "./dist/cjs/*",
    "./es/*": "./dist/es/*",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qlover/fe-base.git",
    "directory": ""
  },
  "files": [
    "dist",
    "templates",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "lib",
    "node lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "create-brain-app": "./dist/index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "test": "vitest run",
    "run:command": "node bin/test.js"
  },
  "devDependencies": {
    "@qlover/env-loader": "latest",
    "@qlover/fe-standard": "latest",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.13.2",
    "release-it": "^17.10.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "commander": "^13.1.0",
    "ignore": "^7.0.3",
    "inquirer": "^12.3.2",
    "ora": "^8.1.1"
  }
}
