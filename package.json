{
  "name": "brainai-frontend",
  "description": "A tools for brainai-frontend",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/qlover/fe-base#readme",
  "author": "qlover",
  "license": "ISC",
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./cjs/*": "./dist/cjs/*",
    "./es/*": "./dist/es/*",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/qlover/fe-base",
    "directory": ""
  },
  "files": [
    "bin",
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "template",
    "brainai-frontend"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {},
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "prettier": "prettier --ignore-path .prettierignore **/*.{js,ts,json,cjs,mjs} --write",
    "lint": "eslint . --fix",
    "clean": "fe-clean",
    "check-packages": "fe-check-packages",
    "commit": "fe-commit",
    "clean-branch": "fe-clean-branch",
    "clean:build": "fe-clean -f packages/*/dist -r",
    "release:create-brain-app": "fe-release -p ./packages/create-brain-app -V -P",
    "release-pr:create-brain-app": "fe-release -p ./packages/create-brain-app -V"
  },
  "devDependencies": {
    "@qlover/env-loader": "latest",
    "@qlover/fe-standard": "latest",
    "@qlover/fe-scripts": "latest",
    "@qlover/scripts-context": "latest",
    "@qlover/fe-utils": "latest",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@testing-library/react": "^16.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.0",
    "rollup": "^4.24.2",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.0",
    "vite": "^5.4.8",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@qlover/fe-utils": "latest"
  },
  "devEngines": {
    "node": ">=18.19.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix --quiet",
      "prettier --write"
    ],
    "packages/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.1.0"
}
